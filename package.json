{
  "name": "gofisch_site-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5",
    "swiper": "4.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start && react-scripts watch-css",
    "build": "react-scripts build && yarn build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
